{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-shopketo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-shopketo/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.png",
              "src/robots.txt",
              "src/sitemap.xml",
              "src/sitemap-landingpage-en-au.xml",
              "src/sitemap-landingpage-en-ca.xml",
              "src/sitemap-landingpage-en-hk.xml",
              "src/sitemap-landingpage-en-mo.xml",
              "src/sitemap-landingpage-en-mx.xml",
              "src/sitemap-landingpage-en-my.xml",
              "src/sitemap-landingpage-en-nz.xml",
              "src/sitemap-landingpage-en-sg.xml",
              "src/sitemap-landingpage-en-us.xml",
              "src/sitemap-landingpage-es-mx.xml",
              "src/sitemap-landingpage-es-us.xml",
              "src/sitemap-landingpage-zh-hans-hk.xml",
              "src/sitemap-landingpage-zh-hans-mo.xml",
              "src/sitemap-landingpage-zh-hans-my.xml",
              "src/sitemap-landingpage-zh-hans-sg.xml",
              "src/sitemap-landingpage-zh-hant-hk.xml",
              "src/sitemap-landingpage-zh-hant-mo.xml",
              "src/sitemap-landingpage-zh-hant-my.xml",
              "src/sitemap-landingpage-zh-hant-sg.xml",
              "src/sitemap-country-index.xml",
              "src/sitemap-all-links-au-index.xml",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/slick-carousel/slick/slick.scss"
            ],
            "scripts": [
              "src/assets/js/jquery-3.4.1.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/iscroll.min.js",
              "src/assets/js/drawer.min.js",
              "src/assets/js/products-body.js",
              "src/assets/js/blog.js",
              "src/assets/js/aos.js",
              "src/assets/js/research.js",
              "src/assets/js/typetext.js",
              "src/assets/js/script.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": ["crypto-js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "demo": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.demo.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-shopketo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-shopketo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-shopketo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-shopketo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-shopketo:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/ng-shopketo/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "optimization": false,
            "sourceMap": true,
            "extractLicenses": false
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true
            },
            "staging": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true
            },
            "demo": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.demo.ts"
                }
              ],
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "ng-shopketo:build",
            "serverTarget": "ng-shopketo:server"
          },
          "configurations": {
            "development": {
              "browserTarget": "ng-shopketo:build:development",
              "serverTarget": "ng-shopketo:server:development"
            },
            "production": {
              "browserTarget": "ng-shopketo:build:production",
              "serverTarget": "ng-shopketo:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-shopketo:build:production",
              "serverTarget": "ng-shopketo:server:production"
            },
            "development": {
              "browserTarget": "ng-shopketo:build:development",
              "serverTarget": "ng-shopketo:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "defaultProject": "ng-shopketo",
  "cli": {
    "analytics": "f5336d7e-e103-4720-9892-c0179ced61f3",
    "defaultCollection": "@angular-eslint/schematics"
  }
}
